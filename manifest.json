{
  "author": "Shashank Shastri",
  "manifest_version": 2,
  "name": "Read My Comments",
  "version": "0.1",
  "description": "An extension to help people confirm their comment before posting it.",
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/lib/jquery-3.4.1.js", "js/app/content_script.js"]
    }
  ],
  "web_accessible_resources": [
    "js/*"
  ]
}
